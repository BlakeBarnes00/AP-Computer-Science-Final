# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  CeasarCypher_config = debug
endif
ifeq ($(config),release)
  CeasarCypher_config = release
endif

PROJECTS := CeasarCypher

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

CeasarCypher:
ifneq (,$(CeasarCypher_config))
	@echo "==== Building CeasarCypher ($(CeasarCypher_config)) ===="
	@${MAKE} --no-print-directory -C . -f CeasarCypher.make config=$(CeasarCypher_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f CeasarCypher.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   CeasarCypher"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"